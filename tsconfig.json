{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "plugins": [
      {
        "transform": "typescript-plugin-styled-components",
        "type": "config",
        "minify": true,
        "ssr": true
      }
    ],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "es5",
  }
}